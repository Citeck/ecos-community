<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN" "http://www.springframework.org/dtd/spring-beans-2.0.dtd">
<beans>

    <!-- ## ## ## ## ## ## ## ## FORM FILTERS (for form-processors) ## ## ## ## ## ## ## ## ## ## -->

    <!-- Attorney Duplicate Form Filter -->
    <bean id="attorneyDuplicatorFilter" class="ru.citeck.ecos.form.AttorneyDuplicateFormFilter"
          parent="baseFormFilter">
        <property name="filterRegistry" ref="duplicatorFilterRegistry" />
        <property name="config">
            <map>
                <entry key="att:attorneyForm">
                    <map>
                        <!-- контент -->
                        <entry key="cm:content"><null/></entry>
                        <!-- регистрационный номер -->
                        <entry key="idocs:registrationNumber"><null/></entry>
                        <!-- дата отзыва -->
                        <entry key="att:cancellationDate"><null/></entry>
                        <!-- дата регистрации -->
                        <entry key="idocs:registrationDate"><null/></entry>
                        <!-- статус -->
                        <entry key="att:status"><null/></entry>
                        <!-- номур заявки -->
                        <entry key="att:number"><null/></entry>
                        <!-- Документ отправлен в филиал/передан инициатору -->
                        <entry key="att:isSentToCompany"><null/></entry>
                        <!-- исполнитель -->
                        <entry key="att:implementer"><null/></entry>
                        <!-- скан копия прикреплена -->
                        <entry key="att:isScanCopyAttached"><null/></entry>
                        <!-- оригинал получен -->
                        <entry key="att:isOriginalReceived"><null/></entry>
                    </map>
                </entry>
            </map>
        </property>
    </bean>

</beans>