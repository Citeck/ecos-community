<webscript>
    <shortname>Upload file</shortname>
    <description>Upload file to a temp container</description>
    <url>/eform/file</url>
    <args>
        <arg>
            <shortname>name</shortname>
            <description>Unique file name, generated by eform file control</description>
        </arg>
        <arg>
            <shortname>file</shortname>
            <description>binary file</description>
        </arg>
    </args>
    <format default="json"/>
    <authentication>user</authentication>
    <transaction>required</transaction>
    <lifecycle>public_api</lifecycle>
</webscript>