<?xml version='1.0' encoding='UTF-8'?>
<component>
    <scope>page</scope>
    <source-id>card-details</source-id>
    <region-id>advanced-associations</region-id>
    <url>/citeck/components/document-associations</url>
    <properties>
        <nodeRef>{nodeRef}</nodeRef>
        <columns>
        	[
        		{ attribute: "cm:title" },
        		{ attribute: "cm:created", formatter: Citeck.format.date('dd.MM.yyyy HH:mm')}
