<?xml version='1.0' encoding='UTF-8'?>

<model name="itmpl:idocsTemplateModel" xmlns="http://www.alfresco.org/model/dictionary/1.0">

	<description>iDocs Template Model</description>
	<author>Citeck</author>
	<version>1.0</version>

	<imports>
		<import uri="http://www.alfresco.org/model/dictionary/1.0" prefix="d" />
		<import uri="http://www.alfresco.org/model/content/1.0" prefix="cm" />
		<import uri="http://www.alfresco.org/model/datalist/1.0" prefix="dl" />
	</imports>

	<namespaces>
		<namespace uri="http://www.citeck.ru/model/idocs-template/1.0" prefix="itmpl" />
	</namespaces>

	<aspects>
		<aspect name="itmpl:hasGeneratedName">
			<properties>
				<property name="itmpl:generatedName">
					<type>d:text</type>
					<index enabled="true">
						<atomic>true</atomic>
						<tokenised>false</tokenised>
					</index>
				</property>
			</properties>
		</aspect>
	</aspects>

</model>
