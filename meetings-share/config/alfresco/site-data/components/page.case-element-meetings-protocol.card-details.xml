<?xml version='1.0' encoding='UTF-8'?>
<component>
    <scope>page</scope>
    <source-id>card-details</source-id>
    <region-id>case-element-meetings-protocol</region-id>
    <url>/citeck/components/child-forms</url>
    <properties>
        <header>cardlet.meetings-protocol.header</header>
        <collapseHeader>true</collapseHeader>
        <nodeRef>{nodeRef}</nodeRef>
        <elementsUrl>citeck/assocs?nodeRef=[[nodeRef]]&amp;assocTypes=meet:childProtocol&amp;addAssocs=false</elementsUrl>
        <elementsPath>children</elementsPath>
        <elementHeader></elementHeader>
        <viewFormUrl>components/form?itemKind=node&amp;itemId=[[nodeRef]]&amp;mode=view</viewFormUrl>
        <editFormUrl>true</editFormUrl>
        <twisterKey>protocol</twisterKey>

        <afterCreate>
            ButtonPanel.removePanelAction('onPanelButtonCreate');
            ButtonPanel.options.args.nodeRef = persistedNode;
            ButtonPanel.renderPanelAction('onPanelButtonEdit');
        </afterCreate>

		<destination>{nodeRef}</destination>
		<calcDestinationChildrenCount>true</calcDestinationChildrenCount>
		<contentType>meet:protocol</contentType>
		<assocType>meet:childProtocol</assocType>
    </properties>
</component>
