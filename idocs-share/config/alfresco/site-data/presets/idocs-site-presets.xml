<presets>
	<preset id="document-site-dashboard">
		<pages>
			<page id="site/${siteid}/dashboard">
				<title>Document Site Dashboard</title>
				<description>Document site's dashboard page</description>
				<template-instance>dashboard-2-columns-wide-right</template-instance>
				<authentication>user</authentication>
				<properties>
					<sitePages>[{"pageId":"documentlibrary"}, {"pageId":"journals"}, {"pageId":"document-types"}]</sitePages>
				</properties>
			</page>
		</pages>
		<components>
			<component>
				<scope>page</scope>
				<region-id>title</region-id>
				<source-id>site/${siteid}/dashboard</source-id>
				<url>/components/title/collaboration-title</url>
			</component>
			<component>
				<scope>page</scope>
				<region-id>navigation</region-id>
				<source-id>site/${siteid}/dashboard</source-id>
				<url>/components/navigation/collaboration-navigation</url>
			</component>
			<component>
				<scope>page</scope>
				<region-id>component-1-1</region-id>
				<source-id>site/${siteid}/dashboard</source-id>
				<url>/components/dashlets/site-search</url>
			</component>
			<component>
				<scope>page</scope>
				<region-id>component-2-1</region-id>
				<source-id>site/${siteid}/dashboard</source-id>
				<url>/components/dashlets/activityfeed</url>
			</component>
			<component>
				<scope>page</scope>
				<region-id>component-2-2</region-id>
				<source-id>site/${siteid}/dashboard</source-id>
				<url>/components/journals2/dashlet</url>
				<properties>
					<journalsListId>site-${siteid}-main</journalsListId>
				</properties>
			</component>
		</components>
	</preset>

	<preset id="file-site-dashboard">
		<pages>
			<page id="site/${siteid}/dashboard">
				<title>File Site Dashboard</title>
				<description>File site's dashboard page</description>
				<template-instance>dashboard-2-columns-wide-right</template-instance>
				<authentication>user</authentication>
				<properties>
					<sitePages>[{"pageId":"documentlibrary"}]</sitePages>
				</properties>
			</page>
		</pages>
		<components>
			<component>
				<scope>page</scope>
				<region-id>title</region-id>
				<source-id>site/${siteid}/dashboard</source-id>
				<url>/components/title/collaboration-title</url>
			</component>
			<component>
				<scope>page</scope>
				<region-id>navigation</region-id>
				<source-id>site/${siteid}/dashboard</source-id>
				<url>/components/navigation/collaboration-navigation</url>
			</component>
			<component>
				<scope>page</scope>
				<region-id>component-1-1</region-id>
				<source-id>site/${siteid}/dashboard</source-id>
				<url>/components/dashlets/site-search</url>
			</component>
			<component>
				<scope>page</scope>
				<region-id>component-2-1</region-id>
				<source-id>site/${siteid}/dashboard</source-id>
				<url>/components/dashlets/activityfeed</url>
			</component>
		</components>
	</preset>
</presets>