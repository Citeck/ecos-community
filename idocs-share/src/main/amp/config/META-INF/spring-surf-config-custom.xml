<alfresco-config>

    <plug-ins>
        <element-readers>
            <element-reader element-name="actionGroups" class="ru.citeck.ecos.surf.config.IdBasedElementReader"/>
            <element-reader element-name="multi-select" class="ru.citeck.ecos.surf.config.IdBasedElementReader"/>
            <element-reader element-name="journals" class="ru.citeck.ecos.surf.config.IdBasedElementReader"/>
            <element-reader element-name="page-titles" class="ru.citeck.ecos.surf.config.IdBasedElementReader"/>
        </element-readers>
    </plug-ins>

    <config evaluator="string-compare" condition="WebFramework">
		<dojo-pages>
			<packages>
			    <package name="citeckHeaderItem" location="js/citeck/header/citeckMenuItem"/>
			    <package name="citeckMenuBarItem" location="js/citeck/menus/citeckMenuBarItem"/>
			    <package name="citeckMenuGroup" location="js/citeck/menus/citeckMenuGroup"/>
			    <package name="citeckMenuItem" location="js/citeck/menus/citeckMenuItem"/>
			</packages>
	    </dojo-pages>
    </config>

</alfresco-config>