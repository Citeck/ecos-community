<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE beans PUBLIC '-//SPRING//DTD BEAN//EN' 'http://www.springframework.org/dtd/spring-beans.dtd'>
<beans>

    <bean id="webscript.ru.citeck.journals2.records.records.post" parent="webscript"
          class="ru.citeck.ecos.journals.webscripts.JournalRecordsPost" />

    <bean id="webscript.ru.citeck.journals2.create-variants.get" parent="webscript"
          class="ru.citeck.ecos.journals.webscripts.CreateVariantsGet">
        <property name="cacheAgeSeconds" value="600" />
    </bean>

    <bean id="webscript.ru.citeck.journals2.config.journal-config.get" parent="webscript"
          class="ru.citeck.ecos.journals.webscripts.JournalConfigGet">
    </bean>

    <bean id="webscript.ru.citeck.journals2.config.journal-gql-schema.get" parent="webscript"
          class="ru.citeck.ecos.journals.webscripts.JournalGqlSchemaGet">
    </bean>

</beans>