<?xml version="1.0" encoding="UTF-8"?>
<webscript>
	<shortname>Node view</shortname>
	<description>Get node view by nodeRef or type name</description>
	<url>/citeck/invariants/view?type={type}</url>
	<url>/citeck/invariants/view?nodeRef={nodeRef}</url>
	<format default="json"/>
	<authentication>user</authentication>
	<transaction allow="readonly">required</transaction>
	<lifecycle>public_api</lifecycle>
</webscript>