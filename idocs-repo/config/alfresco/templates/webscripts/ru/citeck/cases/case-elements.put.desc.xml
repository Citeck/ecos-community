<webscript>
	<shortname>Add case elements</shortname>
	<description>Add specified elements to the case with specified element type</description>
	<url>/citeck/case/elements</url>
	<format default="json"/>
	<authentication>user</authentication>
	<transaction>required</transaction>
	<args>
		<arg>
			<shortname>caseNode</shortname>
			<description>nodeRef of case</description>
		</arg>
		<arg>
			<shortname>elements</shortname>
			<description>array of nodeRefs of elements to add</description>
		</arg>
		<arg>
			<shortname>elementType</shortname>
			<description>name of case element type configuration node</description>
		</arg>
	</args>
</webscript>
