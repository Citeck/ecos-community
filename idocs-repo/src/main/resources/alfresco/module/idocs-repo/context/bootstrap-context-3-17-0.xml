<?xml version='1.0' encoding='UTF-8'?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

    <bean id="idocs.module-component.newFormsDebugEnabled" parent="idocs.import-spaces.3-2-0">
        <property name="name" value="new-forms-debug-enabled"/>
        <property name="description" value="new-forms-debug-enabled"/>
        <property name="bootstrapViews">
            <list>
                <props>
                    <prop key="path">/app:company_home/app:dictionary</prop>
                    <prop key="location">alfresco/templates/bootstrap/ru/citeck/ecosConfig.xml</prop>
                    <prop key="messages">alfresco/module/idocs-repo/bootstrap/configs/new-forms-logging-enabled</prop>
                    <prop key="encoding">UTF-8</prop>
                </props>
            </list>
        </property>
    </bean>
</beans>
