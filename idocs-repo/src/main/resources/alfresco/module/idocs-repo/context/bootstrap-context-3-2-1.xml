<?xml version='1.0' encoding='UTF-8'?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
        http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

    <bean id="idocs.import-spaces.3-2-1" abstract="true" parent="idocs.import-spaces">
        <property name="sinceVersion" value="3.2.1" />
        <property name="appliesFromVersion" value="3.2.1" />
    </bean>

    <bean id="idocs.import-scripts.3-2-0" abstract="true" parent="idocs.import-scripts">
        <property name="sinceVersion" value="3.2.1" />
        <property name="appliesFromVersion" value="3.2.1" />
    </bean>

    <bean id="idocs.script.case-statuses" parent="idocs.import-scripts">
        <property name="executeOnceOnly" value="false" />
        <property name="name" value="add-contracts-case-statuses" />
        <property name="description" value="Create or update case statuses" />
        <property name="bootstrapScript" value="alfresco/module/idocs-repo/bootstrap/scripts/case-statuses.js"/>
    </bean>

    <bean id="idocs.patch.move-case-statuses-creation-to-idocs" parent="ru.citeck.ecos.patch.MoveComponentsPatch">
        <property name="id" value="idocs.patch.move-case-statuses-creation-to-idocs" />
        <property name="description" value="idocs.patch.move-case-statuses-creation-to-idocs.description" />
        <property name="fromModuleId" value="contracts-ent-repo" />
        <property name="toModuleId" value="idocs-repo" />
        <property name="transitions">
            <list>
                <props>
                    <prop key="from">contracts-ent.script.case-statuses</prop>
                    <prop key="to">idocs.script.case-statuses-migration</prop>
                </props>
            </list>
        </property>
    </bean>

</beans>