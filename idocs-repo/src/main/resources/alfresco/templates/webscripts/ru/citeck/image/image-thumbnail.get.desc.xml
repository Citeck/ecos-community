<webscript>
    <shortname>Get image thumbnail</shortname>
    <description>thumbnail image will be cached as child of original image node</description>
    <url>/citeck/ecos/image/thumbnail?imageRef={imageRef}&amp;width={width}&amp;height={height}&amp;stretch={stretch}&amp;cached={cached}&amp;attach={attach}</url>
    <format default="">argument</format>
    <authentication>user</authentication>
    <transaction allow="readonly">required</transaction>
</webscript>
