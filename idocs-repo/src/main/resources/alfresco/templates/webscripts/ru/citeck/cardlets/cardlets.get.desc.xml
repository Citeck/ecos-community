<webscript>
    <shortname>cardlets for the document</shortname>
    <url>/citeck/cardlets?nodeRef={nodeRef}</url>
    <url>/citeck/card/cardlets?nodeRef={nodeRef}&amp;withModes={withModes}</url>
    <format default="json"/>
    <authentication>user</authentication>
    <transaction allow="readonly">required</transaction>
    <cache>
        <never>false</never>
        <public>false</public>
        <mustrevalidate/>
    </cache>
</webscript>
