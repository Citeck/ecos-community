<webscript>
	<shortname>Remove case elements</shortname>
	<description>Remove specified elements from the case with specified element type</description>
	<url>/citeck/case/elements?caseNode={caseNode}&amp;nodeRef={nodeRef}&amp;elementType={elementType}</url>
	<format default="json"/>
	<authentication>user</authentication>
	<transaction>required</transaction>
	<args>
		<arg>
			<shortname>caseNode</shortname>
			<description>nodeRef of case</description>
		</arg>
		<arg>
			<shortname>nodeRef</shortname>
			<description>nodeRef of element to be removed</description>
		</arg>
		<arg>
			<shortname>elementType</shortname>
			<description>name of case element type configuration node</description>
		</arg>
	</args>
</webscript>
